/* QWidget
--------------------------------------------------------- */
QWidget {
    background: #313;
}
QWidget#titleBar {
    background: #202;
}
QWidget#window {
    border: 1px solid #101;
}
QWidget#topWidgetDialog {
    background: #202;
    border-bottom: 1px solid #1b001b;
}
QWidget#selectPlWidget {
    background: #202;
    border: 1px solid #1b001b;
}
QWidget#configContent {
    border: 1px solid #1b001b;
}
QWidget#radioInfoWidget {
    background: #202;
    border-top: 1px solid #1b001b;
    border-bottom: 1px solid #422042;
}
QWidget#statusWidget,
QWidget#tracksWidget {
    background: #202;
    border: 1px solid #1b001b;
}

/* QMenuBar
--------------------------------------------------------- */
QMenuBar {
    background: #202;
    border-bottom: 1px solid #1b001b;
}







